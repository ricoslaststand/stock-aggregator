{
  "name": "stock-aggregator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "aggregate": "npm run build && dist/index.js",
    "migrate": "NODE_ENV=staging knex migrate:up",
    "lint": "npx @biomejs/biome lint ./src",
    "format": "npx @biomejs/biome format --write ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "precommit": "lint-staged"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supercharge/promise-pool": "^2.4.0",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "dotenv": "^16.4.1",
    "knex": "^2.4.2",
    "papaparse": "^5.4.1",
    "pg": "^8.10.0",
    "simple-statistics": "^7.8.3",
    "sleep-promise": "^9.1.0",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.1",
    "xlsx": "^0.18.5",
    "yahoo-finance2": "^2.3.10"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@types/jest": "^29.5.11",
    "@types/knex": "^0.16.1",
    "@types/papaparse": "^5.3.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true",
      "biome check --apply --no-errors-on-unmatched",
      "biome check --apply --organize-imports-enabled=false --no-errors-on-unmatched",
      "biome check --apply-unsafe --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched",
      "biome lint --apply --no-errors-on-unmatched"
    ],
    "*": [
      "biome check --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  }
}
